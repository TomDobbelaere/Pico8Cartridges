pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
cam = {}
cam.x = 0
cam.y = 0


function _draw()
	cls()
	
	--camera(cam.x / 2, cam.y / 2)
	--map(0, 48, cam.x, cam.y, 16, 16)
	
	--camera(cam.x/1.2, cam.y/1.2)
	
	camera(cam.x,cam.y)
	
	for y=-1,16 do
		for x=-1,16 do
			spr(2,cam.x+x*8+cam.x/-2%8,cam.y+y*8+cam.y/-2%8)
		end
	end
	
	map(0,0,0,0,128,32)
end

function _update60()
	if (btn(1)) cam.x += 1
	if (btn(0)) cam.x -= 1
	if (btn(3)) cam.y += 1
	if (btn(2)) cam.y -= 1
end



